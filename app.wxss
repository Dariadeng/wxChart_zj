.weui-loadmore__tips{
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
  color: rgba(0,0,0,.5);
}
.weui-loadmore__tips_in-line {
  position: relative;
  top: -.9em;
  padding: 0 .55em;
