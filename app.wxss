.flex_center{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  align-items: center;
}
.flex_between{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
}
.flex_start{
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
  align-items: center;
}
.flex_end{
  display: flex;
  display: -webkit-flex;
  justify-content: flex-end;
  align-items: center;
}
.title-cell_in-access{
  padding-right: 50rpx;
  position: relative;
}
.title-cell_in-access::after{
  content: " ";
  display: inline-block;
  height: 16rpx;
  width: 16rpx;
  border-width: 4rpx 4rpx 0  0;
  border-color: #b2b2b2;
  border-style: solid;
  -webkit-transform: matrix(.71,.71,-.71,.71,0,0);
  transform: matrix(.71,.71,-.71,.71,0,0);
  /* position: relative; */
  top: -4rpx;
  position: absolute;
  top: 50%;
  margin-top: -10rpx;
  right: 20rpx;
}
/* 公司名称 单行省略号 */
.one_line_omit{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
/* 单体企业/企业集合 */
.title_block{
    display: inline-block;
    width: 100rpx;
    text-align: center;
    margin-left: 16rpx;
    padding: 2rpx 12rpx;
    background:rgba(0,0,0,0.2);
    border-radius:2px;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    line-height:34rpx;
    white-space: nowrap;
}
.type_block{
    display: inline-block;
    text-align: center;
    margin-left: 16rpx;
    padding: 2rpx 12rpx;
    border-radius:2px;
    font-size:24rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    line-height:34rpx;
    white-space: nowrap;
}
.block_orange{
  width: 100rpx;
  color:#FC7D81;
  background:rgba(252,124,129,0.09);
}
.block_blue{
  width: 100rpx;
  color:#40A9FF;
  background:rgba(64,169,255,0.09);
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: space-between; */
  padding: 190rpx 42rpx;
  box-sizing: border-box;
} 
.company-info{
    padding: 20rpx 30rpx;
}
.content-mian-bd{
  display: flex;
}
.content-mian-bd-in{
  margin-bottom: 20rpx;
}
.title .title-img{
  display: flex;
  align-items: center;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
}
.title .title-cell{
  text-align: right;
  color: rgba(0,0,0,.5);
}
.title-img .images1{
   width: 32rpx;
   height: 32rpx;
   margin-right: 16rpx;
}
.text{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  line-height:40rpx;
  color:rgba(30,30,30,0.5);
  white-space: nowrap;
}
.images1{
   width: 32rpx;
   height: 32rpx;
   margin-right: 16rpx;
}
.content-mian-bd .main-item,
.content-mian-bd-in .main-item{
  width: 50%;
  padding: 10rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  line-height:40rpx;
}
.main-cell{
  padding: 0 10rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  line-height:40rpx;
}
.split-line{
  width:2rpx;
  height:54rpx; 
  background-color: #F3F3F3;
}
.split-line-bt{
  border-bottom: 2rpx solid #F3F3F3;
}
.loading,
.noData{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
